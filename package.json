{
  "name": "jest-performance",
  "version": "1.0.0",
  "description": "Benchmark JS integration for Jest",
  "keywords": [
    "js",
    "javascript",
    "benchmark",
    "performance"
  ],
  "author": "Ermes Enea Colella",
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib --extensions '.ts,.tsx'",
    "ts-check": "tsc --noEmit",
    "clean": "rimraf lib",
    "publish": "npm run clean && npm run build && np"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "cli-table": "^0.3.1",
    "colors": "^1.4.0",
    "ramda": "^0.27.0",
    "simple-git": "^2.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "np": "^6.3.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eecolella/jest-performance"
  }
